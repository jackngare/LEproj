@{
    ViewBag.Title = "Dashboard";
}

<div class="wrapper wrapper-content animated fadeInUp">
    <div class="row" >
        <div class="col-lg-3">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Species</h5>
                </div>
                <div class="ibox-content">
                    <h1 class="no-margins">6</h1>
                    <small>Total No. Species</small>
                </div>
            </div>
        </div>
        <div class="col-lg-3">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Animals</h5>
                </div>
                <div class="ibox-content">
                    <h1 class="no-margins">275</h1>
                    <small>No. of Animals sighted</small>
                </div>
            </div>
        </div>
        <div class="col-lg-3">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <span class="label label-primary pull-right">Weekly</span>
                    <h5>Observations</h5>
                </div>
                <div class="ibox-content">
                    <h1 class="no-margins">106,120</h1>
                    <div class="stat-percent font-bold text-navy">44% <i class="fa fa-level-up"></i></div>
                    <small>Total No. Observations</small>
                </div>
            </div>
        </div>
        <div class="col-lg-3">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <span class="label label-danger pull-right">Annual</span>
                    <h5>Observations</h5>
                </div>
                <div class="ibox-content">
                    <h1 class="no-margins">80,600</h1>
                    <div class="stat-percent font-bold text-danger">38% <i class="fa fa-level-down"></i></div>
                    <small>Total Annual No. Observations</small>
                </div>
            </div>
        </div>
    </div>

</div>


<div id="mapArea" style="width:100%; height:72%;"></div>


<script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?key=AIzaSyD8fS3hJwOoP9lir5q-hGUanxUV0mKK0P0&libraries=places&sensor=false"></script>

<script type="text/javascript">
    window.onload = function () {
        initialize();
    }
</script>
